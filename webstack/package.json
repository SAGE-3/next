  "description": "Directory containing an NX setup, for the 'homebase' web server and the React web applications such as 'CollaborationSpace' (the main collaboration canvas) but might contain also other web apps during development (admin, monitoring, login, ...). Could also contain other API services if needed.",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "buildall": "nx run-many --target=build --all",
    "homebase": "nx serve homebase",
    "webapp": "nx serve webapp --host=0.0.0.0",
    "stage": "rimraf dist && yarn build homebase && yarn build webapp && mkdir -p dist/apps/homebase/webapp && cp -r dist/apps/webapp/* dist/apps/homebase/webapp",
    "prod": "rimraf dist && yarn build homebase --prod && yarn build webapp --prod && mkdir -p dist/apps/homebase/webapp && cp -r dist/apps/webapp/* dist/apps/homebase/webapp",
    "newapp": "nx workspace-generator newapp",
    "regen": "nx workspace-generator regen"
  },
